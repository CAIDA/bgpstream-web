{% extends 'CAIDABGPStreamWebHomepageBundle:Default:docs/base.html.twig' %}

{% block styleblock %}
    {{ parent() }}
    {% stylesheets
    '@CAIDABGPStreamWebHomepageBundle/Resources/public/libs/sphinx-docs/css/basic.css'
    '@CAIDABGPStreamWebHomepageBundle/Resources/public/libs/sphinx-docs/css/pygments.css'
    '@CAIDABGPStreamWebHomepageBundle/Resources/public/libs/sphinx-docs/css/bootstrap-sphinx.css'
    '@CAIDABGPStreamWebHomepageBundle/Resources/public/css/sphinx-docs.css'
    filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}

{% block head_js %}
{% endblock %}

{% block slimbody %}
    <div class="page-header">
        <h1>{{ filename }}</h1>
    </div>
    {% include file_path %}
{% endblock %}

